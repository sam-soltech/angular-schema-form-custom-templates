{
    "compilerOptions": {
        "target": "es5",
        "module": "system",
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": false,
        "sortOutput": true
    },
    "exclude": [
      "node_modules"
    ],
    "files": [
      "../../typings/index.d.ts",
      "./app.ts",
      "./controllers/FormDirectiveController.ts",
      "./directives/FormDirective.ts"
    ]
}
